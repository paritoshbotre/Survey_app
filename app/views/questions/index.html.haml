%h2 Questions
%h3
  - @question.each do |q|
    .questions
      = link_to q.description, survey_question_path(@survey, q)
      %br
      =# link_to "Edit Question", edit_survey_question_path(@survey,q)
      =# link_to "Delete Question", survey_question_path(@survey, q), method: :delete
      %br
%hr
= link_to "Create Questions", new_survey_question_path(@survey), { class: "button", style: "text-decoration:none" }
%br
= link_to "Back to Survey", survey_path(@survey), { class: "button", style: "text-decoration:none" }